{
  "name": "vue-lp.test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix",
    "lint:stylelint": "stylelint --cache \"src/**/*.{vue,scss}\" && exit 0",
    "lint:eslint": "eslint --cache \"src/**/*.{js,ts,vue}\"",
    "prepush": "cross-env NODE_ENV=production npm run lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.2",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-scss": "^3.17.0",
    "url-pattern": "^1.0.3",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-push": "npm run prepush"
  }
}
